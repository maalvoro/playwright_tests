{
  "name": "playwright_tests",
  "version": "1.0.0",
  "description": "Este proyecto contiene las pruebas E2E para la aplicaci√≥n Happy Testing usando Playwright.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:auth": "npx playwright test --project=auth",
    "test:chromium": "npx playwright test --project=chromium",
    "test:report": "npx playwright show-report",
    "mcp:validate": "npx playwright test --config=playwright.config.ts --dry-run",
    "mcp:generate": "node mcp-executor.js",
    "mcp:agent": "node mcp-executor.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maalvoro/playwright_tests.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/maalvoro/playwright_tests/issues"
  },
  "homepage": "https://github.com/maalvoro/playwright_tests#readme",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1"
  }
}
